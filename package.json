{
  "name": "register-accounts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack --config webpack.config.js",
    "start": "electron .",
    "dist": "npm run build && electron-builder",
    "start:dev": "npm run build && electron .",
    "clean": "rimraf dist"
  },
  "build": {
    "appId": "com.example.registrationtool",
    "productName": "批量注册工具",
    "compression": "maximum",
    "files": [
      "dist/**/*",
      "package.json",
      "assets/**/*",
      "public/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "chrome-launcher": "^1.1.2",
    "nanoid": "^5.0.9",
    "os": "^0.1.2",
    "p-limit": "^6.1.0",
    "puppeteer-core": "^23.10.4",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "rimraf": "^6.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
